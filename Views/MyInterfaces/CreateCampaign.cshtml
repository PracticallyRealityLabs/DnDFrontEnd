@model IEnumerable<DnDFrontEnd.ViewModels.UserCampaignViewModel>;
@{
    ViewData["Title"] = "CreateCampaign";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Campaign Created</h1>
<h4>Submit the Email of a User You Would Like to Invite</h4>
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateCampaign">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Add User" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<p>Return to Your Campaigns</p>
<button type="button" onclick="location.href = 'https://localhost:44369/MyInterfaces/UserCampaigns'"> Party Time </button>

